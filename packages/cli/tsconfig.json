{
  // "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2022",
    "baseUrl": ".",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "moduleSuffixes": ["", ".js", ".jsx", ".ts", ".tsx"],

    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "declaration": true,
    "emitDeclarationOnly": true,
    "outDir": "./dist/types"
  },
  "files": ["./global.d.ts"],
  "include": ["./**/*.ts", "vite.config.ts"]
}
