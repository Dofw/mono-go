{
  "name": "@dawnll/dlc",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.1.1",
  "description": "command dlc, a cli tool",
  "author": "dawnl",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "main": "./index.ts",
  "module": "./index.ts",
  "types": "./index.d.ts",
  "bin": {
    "dlc": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "build:vite": "vite build",
    "build:rollup": "rollup -c",
    "dlc": "node ./dist/index.cjs",
    "exec": "node --experimental-specifier-resolution=node --loader=ts-node/esm index.ts"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@types/inquirer": "^9.0.7",
    "adm-zip": "^0.5.12",
    "chalk": "^5.3.0",
    "cli-spinners": "^3.0.0",
    "commander": "^12.0.0",
    "crypto-js": "^4.2.0"
  }
}
