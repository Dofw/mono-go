{
  "name": "@dawnll/dlc",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.1.4",
  "description": "command dlc, a cli tool",
  "author": "dawnl",
  "license": "ISC",
  "keywords": [],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "default": "./dist/index.cjs"
      }
    },
    "./types/*": {
      "types": "./types/*"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./index.ts",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "dlc": "./dist/index.mjs"
  },
  "files": [
    "dist"
  ],

  "engines": {
    "node": ">=20.14.0"
  },
  "scripts": {
    "build:vite": "vite build",
    "build:rollup": "rollup -c -w",
    "tsc": "tsc --build --project tsconfig.json",
    "dlc": "NODE_OPTIONS='--trace-deprecation' node ./dist/index.mjs",
    "exec": " node --experimental-specifier-resolution=node --loader=ts-node/esm index.ts"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.1",
    "@types/inquirer": "^9.0.7",
    "adm-zip": "^0.5.12",
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "crypto-js": "^4.2.0",
    "download-git-repo": "^3.0.2",
    "electron": "^30.0.6",
    "ora": "^8.0.1",
    "prompts": "^2.4.2"
  }
}
